#include <stdio.h>

int main() 
{
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    
    printf("Twice the value of entered number is : %d " , 2*num );
    
    return 0;
}   

/*
OUTPUT

Enter a number: 70
Twice the value of entered number is : 140 

=== Code Execution Successful ===

*/
