#include <stdio.h>

int main() 
{
    int i;
    printf("Enter the value of i = ");
    scanf("%d", &i);
    
    
    for (i ; i <= 10; i++) 
    {
        if (i % 2 == 0) // If the number is even, skip the rest of                the loop and continue with the next                                  iteration
            continue;
    
        printf("%d\n", i);   // This will only print odd numbers
    }
    return 0;
}


/* Output

1
3
5
7
9

== Code Execution Successful===  */
